#include <stdio.h>
#include "xparameters.h"
#include "xil_cache.h"
#include <time.h>
int main() 
{
   Xil_ICacheEnable();
   Xil_DCacheEnable();
   print("---Entering main---\n\r");
   
   unsigned int* ptr = (unsigned int*) 0x80000000;
   for(int i = 0; i < 65536; ++i) {
	   ptr[i] = i;
   }
   xil_printf("time = %f\n\r", (double)(end - start) / 1000000000);
   for(int i = 0; i < 65536; ++i) {
	   if(ptr[i] != i) {
		   xil_printf("test ddt2 0x%d = %d failed!\n\r", i, ptr[i]);
		   continue;
	   }
   }

   print("---Exiting main---\n\r");
   Xil_DCacheDisable();
   Xil_ICacheDisable();
   return 0;
}
